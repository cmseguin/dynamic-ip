{
    "extends": "eslint:all",
    "env": {
        "node": true,
        "es6": true,
        "mocha": true
    },
    "rules": {
        "brace-style": 0,
        "capitalized-comments": 0,
        "consistent-return": 0,
        "dot-location": 0,
        "id-length": 0,
        "line-comment-position": 0,
        "lines-around-comment":0,
        "max-len": 0,
        "max-params": 0,
        "max-statements": 0,
        "no-continue": 0,
        "no-empty-function": 0,
        "no-inline-comments": 0,
        "no-labels": 0,
        "no-magic-numbers": 0,
        "no-plusplus": 0,
        "no-sync": 0,
        "no-underscore-dangle": 0,
        "object-curly-spacing": 0,
        "object-curly-newline": 0,
        "one-var": ["error", "never"],
        "padded-blocks": 0,
        "quotes": ["error", "single", { "allowTemplateLiterals": true }],
        "sort-keys": 0,
        "spaced-comment": 0,
        "strict": 0
    }
}
